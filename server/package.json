{
  "name": "cypress-pop-api",
  "version": "1.0.0",
  "description": "API server for cypress-pop image downloader",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:watch": "nodemon --watch . --ext js,json --ignore node_modules/ --exec 'node server.js'",
    "test:logging": "node test-logging.js",
    "test:browser": "node test-browser.js",
    "diagnose": "node diagnose-docker.js",
    "logs": "node scripts/view-logs.js",
    "logs:errors": "node scripts/view-logs.js --level ERROR",
    "logs:download": "node scripts/view-logs.js --context Download"
  },
  "keywords": ["api", "puppeteer", "image-downloader"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "puppeteer": "^22.13.1",
    "axios": "^1.7.2",
    "node-fetch": "^3.3.2",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
} 